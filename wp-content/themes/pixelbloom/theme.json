{
  "$schema": "https://schemas.wp.org/wp/6.4/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#0073e6",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#6c757d",
          "name": "Secondary"
        },
        {
          "slug": "accent",
          "color": "#ff6b6b",
          "name": "Accent"
        },
        {
          "slug": "background",
          "color": "#ffffff",
          "name": "Background"
        },
        {
          "slug": "text",
          "color": "#333333",
          "name": "Text"
        }
      ],
      "gradients": [
        {
          "slug": "primary-to-accent",
          "gradient": "linear-gradient(135deg, #0073e6 0%, #ff6b6b 100%)",
          "name": "Primary to Accent"
        }
      ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "spacing": {
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Poppins",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/poppins/poppins-400-normal.woff2"
              ]
            },
            {
              "fontFamily": "Poppins",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/poppins/poppins-700-normal.woff2"
              ]
            },
            {
              "fontFamily": "Poppins",
              "fontStyle": "normal",
              "fontWeight": "900",
              "src": [
                "file:./assets/fonts/poppins/poppins-900-normal.woff2"
              ]
            },
            {
              "fontFamily": "Poppins",
              "fontStyle": "normal",
              "fontWeight": "200",
              "src": [
                "file:./assets/fonts/poppins/poppins-200-normal.woff2"
              ]
            }
          ],
          "fontFamily": "Poppins, sans-serif",
          "name": "Poppins",
          "slug": "poppins"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Nunito",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": [
                "file:./assets/fonts/nunito/nunito-400-normal.woff2"
              ]
            },
            {
              "fontFamily": "Nunito",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": [
                "file:./assets/fonts/nunito/nunito-700-normal.woff2"
              ]
            },
            {
              "fontFamily": "Nunito",
              "fontStyle": "normal",
              "fontWeight": "200",
              "src": [
                "file:./assets/fonts/nunito/nunito-200-normal.woff2"
              ]
            },
            {
              "fontFamily": "Nunito",
              "fontStyle": "normal",
              "fontWeight": "900",
              "src": [
                "file:./assets/fonts/nunito/nunito-900-normal.woff2"
              ]
            }
          ],
          "fontFamily": "Nunito, sans-serif",
          "name": "Nunito",
          "slug": "nunito"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small"
        },
        {
          "slug": "medium",
          "size": "1rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "1.125rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "1.5rem",
          "name": "Extra Large"
        },
        {
          "slug": "xx-large",
          "size": "2rem",
          "name": "XX Large"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--text)"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--background)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontSize": "1.25rem",
          "fontStyle": "normal",
          "fontWeight": "700",
          "lineHeight": "1.6"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--accent)"
          }
        }
      },
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontSize": "2rem",
          "fontStyle": "normal",
          "fontWeight": "700",
          "lineHeight": "1.6"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontSize": "1.8rem",
          "fontStyle": "normal",
          "fontWeight": "700",
          "lineHeight": "1.6"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--nunito)",
          "fontSize": "1.5rem",
          "fontStyle": "normal",
          "fontWeight": "900",
          "lineHeight": "1.6"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontSize": "1.25rem",
          "fontStyle": "normal",
          "fontWeight": "900",
          "lineHeight": "1.6"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--nunito)",
          "fontSize": "2.25rem",
          "fontStyle": "normal",
          "fontWeight": "700",
          "lineHeight": "1.6"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)",
          "fontSize": "1rem",
          "fontStyle": "normal",
          "fontWeight": "700",
          "lineHeight": "1.6"
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--poppins)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        "typography": {
          "lineHeight": "1.6"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--accent)"
          }
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--nunito)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.6"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ]
}